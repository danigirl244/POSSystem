<Window x:Class="pointofsale_application.ActivatePopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pointofsale_application"
        mc:Ignorable="d"
        Title="ActivatePopUp" Height="173.11" Width="300">
    <Grid>
        <Label x:Name="ActiPrompt" Content="Are you sure you want to reactivate/deactivate?" HorizontalAlignment="Left" Margin="19,24,0,0" VerticalAlignment="Top" Height="29"/>
        <Button x:Name="YButton" Content="Yes" HorizontalAlignment="Left" Margin="48,93,0,0" VerticalAlignment="Top" Width="75" Background="#464543"/>
        <Button x:Name="NButton" Content="No" HorizontalAlignment="Left" Margin="183,93,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.359,-1.863" Background="#464543"/>

    </Grid>
</Window>
