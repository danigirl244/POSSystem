<Window x:Class="pointofsale_application.DeletePopUp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pointofsale_application"
        mc:Ignorable="d"
        Title="DeletePopUp" Height="173.11" Width="300">
    <Grid>
        <Label x:Name="DeletePrompt" Content="Are you sure you want to delete this user?" HorizontalAlignment="Left" Margin="33,25,0,0" VerticalAlignment="Top" Height="29"/>
        <Button x:Name="YButton" Content="Yes" HorizontalAlignment="Left" Margin="55,95,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="NButton" Content="No" HorizontalAlignment="Left" Margin="163,95,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
