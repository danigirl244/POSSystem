                    <TextBox Name="vodkaQuant" Width="125" Height="25" FontSize="16" Margin="0,0,30,0" BorderBrush="Black">Quantity:</TextBox>
                    <TextBox Name="vodkaDes" Width="200" Height="75" FontSize="16" BorderBrush="Black">Description:</TextBox>
                </StackPanel>
            </TabItem>
            <TabItem Header="Whiskey" Name="whiskey" Height="50" FontSize="18">
