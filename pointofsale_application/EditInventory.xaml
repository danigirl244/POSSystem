<Window x:Class="pointofsale_application.EditInventory"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pointofsale_application"
        mc:Ignorable="d"
        Title="EditInventory" Height="610" Width="960">
    <Grid Background="#787A79">
        <Button Name="AddProduct" Content="Add Product" FontSize="22" Foreground="#B1B7B7" Background="#464543" Width="175" Height="65" Margin="767,41,10,465"></Button>
        <TabControl x:Name="Products" TabStripPlacement="Left" Margin="0,120,0,0" Height="450">
            <TabItem Header="Beer" Name="beer" Height="50" FontSize="18">
                <Border Background="White" BorderThickness="5" BorderBrush="#464543" Margin="17,130,17,130">
                    <StackPanel Orientation="Horizontal" Margin="15">
                        <TextBox Name="beerID" Width="75" Height="25" FontSize="16" Margin="0,0,30,0" BorderBrush="Black">ID:</TextBox>
                        <TextBox Name="beerName" Width="205" Height="25" FontSize="16" Margin="0,0,30,0" BorderBrush="Black">Name:</TextBox>
                        <TextBox Name="beerPrice" Width="111" Height="25" FontSize="16" Margin="0,0,30,0" BorderBrush="Black">Price: $</TextBox>
                        <TextBox Name="beerQuant" Width="125" Height="25" FontSize="16" Margin="0,0,30,0" BorderBrush="Black">Quantity:</TextBox>
                        <TextBox Name="beerDes" Width="154" Height="75" FontSize="16" BorderBrush="Black" Margin="0,34">Description:</TextBox>
                    </StackPanel>
                </Border>
            </TabItem>
            <TabItem Header="Vodka" Name="vodka" Height="50" FontSize="18">
